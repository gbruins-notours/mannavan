{
  "name": "mannavan",
  "version": "1.0.0",
  "description": "My unreal Nuxt.js project",
  "author": "greg bruins",
  "engines": {
    "node": "8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gbruins-notours/mannavan"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/start.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/start.js",
    "start-local": "cross-env NODE_ENV=production node --require dotenv/config server/start.js",
    "knex:migrate": "node ./node_modules/knex/bin/cli.js migrate:latest",
    "knex:seed": "node --require dotenv/config ./node_modules/knex/bin/cli.js seed:run",
    "test:client": "npm run unit && npm run e2e",
    "test:server": "NODE_ENV=test lab -a code -v --leaks test/server",
    "test:e2eOLD": "node test/client/e2e/runner.js",
    "e2e": "node ./node_modules/testcafe/bin/testcafe.js \"saucelabs:Chrome@beta:Windows 10\" test/client/e2e --app \"npm start\" --app-init-delay 10000",
    "e2e-local": "NODE_ENV=development node --require dotenv/config ./node_modules/testcafe/bin/testcafe.js firefox test/client/e2e --app \"npm run dev\" --app-init-delay 10000",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "hash": "node ./hash.js",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.1.1",
    "accounting": "^0.4.1",
    "auth0-lock": "^11.7.2",
    "aws-sdk": "^2.254.1",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "bookshelf": "^0.13.3",
    "bookshelf-modelbase": "^2.10.4",
    "bookshelf-uuid": "^1.0.0",
    "boom": "^7.2.0",
    "braintree": "^2.11.0",
    "braintree-web": "^3.34.0",
    "bugsnag": "^2.4.1",
    "bugsnag-js": "^4.7.1",
    "bugsnag-vue": "^1.0.1",
    "bulma": "^0.3.2",
    "code": "^5.2.0",
    "confidence": "^3.0.2",
    "cookie": "^0.3.1",
    "dotenv": "^6.0.0",
    "element-ui": "^2.4.1",
    "file-type": "^7.7.1",
    "fs": "0.0.1-security",
    "glue": "^5.0.0",
    "handlebars": "^4.0.11",
    "hapi-nuxt": "^1.0.1",
    "hoek": "^5.0.3",
    "inert": "^5.1.0",
    "joi": "^13.4.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.2.2",
    "jwt-decode": "^2.2.0",
    "knex": "^0.14.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfinite": "^3.3.2",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "mailgun-js": "^0.18.0",
    "node-sass": "^4.9.0",
    "nodemailer": "^4.6.5",
    "nuxt": "^1.4.1",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "promise.prototype.finally": "^3.1.0",
    "pug": "^2.0.3",
    "query-string": "^6.1.0",
    "r7insight_node": "^1.8.1",
    "sass-loader": "^7.0.3",
    "sharp": "^0.20.3",
    "uuid": "^3.2.1",
    "v-img": "^0.2.0",
    "vision": "^5.3.3",
    "vue-carousel": "^0.8.1",
    "vue-i18n": "^7.8.0",
    "vue-social-sharing": "^2.3.3",
    "vue-youtube-embed": "^2.1.3",
    "vuelidate": "^0.7.4",
    "vuex-router-sync": "^5.0.0",
    "winston": "^2.4.2",
    "wreck": "^14.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.6",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.5.0",
    "faker": "^4.1.0",
    "lab": "^15.5.0",
    "nodemon": "^1.17.5",
    "testcafe": "^0.20.3",
    "testcafe-browser-provider-saucelabs": "^1.3.0",
    "testcafe-vue-selectors": "^3.0.0"
  }
}
