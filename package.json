{
  "name": "mannavan",
  "version": "1.0.0",
  "description": "Vintage. Racing. An apparel company",
  "author": "goBreadVan.com",
  "engines": {
    "node": "10.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gbruins-notours/mannavan"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon shopBac/server/start.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node shopBac/server/start.js",
    "start-local": "cross-env NODE_ENV=production node --require dotenv/config shopBac/server/start.js",
    "knex:migrate": "node ./shopBac/node_modules/knex/bin/cli.js migrate:latest",
    "knex:seed": "node --require dotenv/config ./shopBac/node_modules/knex/bin/cli.js seed:run",
    "test:client": "npm run unit && npm run e2e",
    "test:server": "NODE_ENV=test lab -a code -v --leaks shopBac/test/server",
    "test:e2eOLD": "node test/client/e2e/runner.js",
    "e2e": "node ./node_modules/testcafe/bin/testcafe.js \"saucelabs:Chrome@beta:Windows 10\" test/client/e2e --app \"npm start\" --app-init-delay 10000",
    "e2e-local": "NODE_ENV=development node --require dotenv/config ./node_modules/testcafe/bin/testcafe.js firefox test/client/e2e --app \"npm run dev\" --app-init-delay 10000",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "hash": "node ./hash.js",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.4",
    "@nuxtjs/pwa": "^2.6.0",
    "accounting": "^0.4.1",
    "auth0-lock": "^11.12.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.3",
    "braintree-web": "^3.41.0",
    "bugsnag-js": "^4.7.3",
    "bugsnag-vue": "^1.0.1",
    "cookie": "^0.3.1",
    "dotenv": "^6.1.0",
    "element-ui": "^2.4.11",
    "hoek": "^6.0.4",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isobject": "^3.0.2",
    "node-sass": "^4.10.0",
    "nuxt": "^2.3.4",
    "path": "^0.12.7",
    "query-string": "^6.2.0",
    "sass-loader": "^7.1.0",
    "uuid": "^3.3.2",
    "v-img": "^0.2.0",
    "vue-carousel": "^0.8.1",
    "vue-i18n": "^8.3.2",
    "vue-json-tree-view": "^2.1.4",
    "vue-social-sharing": "^2.3.3",
    "vue-youtube-embed": "^2.2.1",
    "vuelidate": "^0.7.4",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^1.18.6",
    "testcafe": "^0.23.2",
    "testcafe-browser-provider-saucelabs": "^1.3.1",
    "testcafe-vue-selectors": "^3.0.0"
  }
}
