    "winston-loggly-bulk": "^2.0.3",
    "wreck": "^14.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "lab": "^15.5.0",
    "nodemon": "^1.18.4",
    "testcafe": "^0.20.5",
    "testcafe-browser-provider-saucelabs": "^1.3.1",
    "testcafe-vue-selectors": "^3.0.0"
  }
}
