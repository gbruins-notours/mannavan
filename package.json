{
  "name": "mannavan",
  "version": "1.0.0",
  "description": "My unreal Nuxt.js project",
  "author": "greg bruins",
  "engines": {
    "node": "8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gbruins-notours/mannavan"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/start.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/start.js",
    "start-local": "cross-env NODE_ENV=production node --require dotenv/config server/start.js",
    "knex:migrate": "node ./node_modules/knex/bin/cli.js migrate:latest",
    "knex:seed": "node --require dotenv/config ./node_modules/knex/bin/cli.js seed:run",
    "test:client": "npm run unit && npm run e2e",
    "test:server": "NODE_ENV=test lab -a code -v --leaks test/server",
    "test:e2eOLD": "node test/client/e2e/runner.js",
    "e2e": "node ./node_modules/testcafe/bin/testcafe.js \"saucelabs:Chrome@beta:Windows 10\" test/client/e2e --app \"npm start\" --app-init-delay 10000",
    "e2e-local": "NODE_ENV=development node --require dotenv/config ./node_modules/testcafe/bin/testcafe.js firefox test/client/e2e --app \"npm run dev\" --app-init-delay 10000",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "hash": "node ./hash.js",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/pwa": "^2.5.0",
    "accounting": "^0.4.1",
    "auth0-lock": "^11.7.2",
    "aws-sdk": "^2.307.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "bookshelf": "^0.13.3",
    "bookshelf-modelbase": "^2.10.4",
    "bookshelf-uuid": "^1.0.0",
    "boom": "^7.2.0",
    "braintree": "^2.12.0",
    "braintree-web": "^3.36.0",
    "bugsnag": "^2.4.3",
    "bugsnag-js": "^4.7.3",
    "bugsnag-vue": "^1.0.1",
    "code": "^5.2.0",
    "confidence": "^3.0.2",
    "cookie": "^0.3.1",
    "dotenv": "^6.0.0",
    "element-ui": "^2.4.8",
    "file-type": "^7.7.1",
    "fs": "0.0.1-security",
    "glue": "^5.0.0",
    "handlebars": "^4.0.11",
    "hapi-auth-basic": "^5.0.0",
    "hapi-nuxt": "^1.0.1",
    "hoek": "^5.0.4",
    "inert": "^5.1.0",
    "joi": "^13.6.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.14.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfinite": "^3.3.2",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "mailgun-js": "^0.18.1",
    "node-sass": "^4.9.3",
    "nodemailer": "^4.6.8",
    "nuxt": "^2.1.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "pug": "^2.0.3",
    "query-string": "^5.1.1",
    "sass-loader": "^7.1.0",
    "sharp": "^0.20.7",
    "uuid": "^3.3.2",
    "uuid-validate": "0.0.3",
    "v-img": "^0.2.0",
    "vision": "^5.4.0",
    "vue-carousel": "^0.8.1",
    "vue-i18n": "^7.8.1",
    "vue-json-tree-view": "^2.1.4",
    "vue-social-sharing": "^2.3.3",
    "vue-youtube-embed": "^2.2.1",
    "vuelidate": "^0.7.4",
    "vuex-router-sync": "^5.0.0",
